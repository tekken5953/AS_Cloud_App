<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Transition
        app:constraintSetEnd="@+id/end"
        app:constraintSetStart="@id/start"
        app:duration="500">

        <OnClick
            app:clickAction="transitionToEnd"
            app:targetId="@id/mainMotionSLideImg"/>
    </Transition>
    <Transition
        app:constraintSetEnd="@+id/end"
        app:constraintSetStart="@id/start"
        app:duration="500">

        <OnSwipe
            app:dragDirection="dragUp"
            app:dragScale="2.5"
            app:maxAcceleration="30"
            app:maxVelocity="30"
            app:touchAnchorId="@id/nested_scrollview"
            app:touchAnchorSide="top" />

        <OnClick
            app:clickAction="transitionToEnd"
            app:targetId="@id/mainMotionSlideGuide"/>


        <KeyFrameSet>
            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="0"
                app:motionTarget="@id/mainWarningBox" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="1"
                app:motionTarget="@id/mainWarningBox" />
            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="0"
                app:motionTarget="@id/nested_subAir_frame" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="1"
                app:motionTarget="@id/nested_subAir_frame" />
            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="0"
                app:motionTarget="@id/mainGpsFix" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="1"
                app:motionTarget="@id/mainGpsFix" />


            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="0"
                app:motionTarget="@id/mainCompareTempTv" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainCompareTempTv" />
            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="0"
                app:motionTarget="@id/mainSensTitle" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainSensTitle" />
            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="0"
                app:motionTarget="@id/mainSensValue" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainSensValue" />
            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="0"
                app:motionTarget="@id/mainMinMaxTr" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainMinMaxTr" />

            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="0"
                app:motionTarget="@id/mainLiveTempValue" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainLiveTempValue" />
            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="0"
                app:motionTarget="@id/mainSensValue" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainSensValue" />
            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="0"
                app:motionTarget="@id/mainSensTitle" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainSensTitle" />
            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="0"
                app:motionTarget="@id/mainLiveTempUnit" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainLiveTempUnit" />


            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainTopBarGpsTitleScroll" />
            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="99"
                app:motionTarget="@id/mainTopBarGpsTitleScroll" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainMinMaxValueC" />
            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="99"
                app:motionTarget="@id/mainMinMaxValueC" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainMinMaxTitleC" />
            <KeyAttribute
                android:alpha="0.8"
                app:curveFit="linear"
                app:framePosition="99"
                app:motionTarget="@id/mainMinMaxTitleC" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainLiveTempValueC" />
            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="99"
                app:motionTarget="@id/mainLiveTempValueC" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainLiveTempTitleC" />
            <KeyAttribute
                android:alpha="0.8"
                app:curveFit="linear"
                app:framePosition="99"
                app:motionTarget="@id/mainLiveTempTitleC" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainSensValueC" />
            <KeyAttribute
                android:alpha="1"
                app:curveFit="linear"
                app:framePosition="99"
                app:motionTarget="@id/mainSensValueC" />
            <KeyAttribute
                android:alpha="0"
                app:curveFit="linear"
                app:framePosition="60"
                app:motionTarget="@id/mainSensTitleC" />
            <KeyAttribute
                android:alpha="0.8"
                app:curveFit="linear"
                app:framePosition="99"
                app:motionTarget="@id/mainSensTitleC" />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/mainTopBarGpsTitleScroll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0"
            android:layout_marginEnd="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/topBarLinear"
            app:layout_constraintBottom_toBottomOf="@+id/topBarLinear"/>

        <Constraint
            android:id="@+id/mainMotionLinear"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/mainGpsTitleTv"
            style="@style/basicTextView"
            android:layout_marginStart="24dp"
            android:layout_marginTop="22dp"
            android:alpha="1"
            android:lineHeight="22dp"
            android:maxEms="10"
            android:fontFamily="@font/spoqa_hansansneo_regular"
            android:gravity="start"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/topBarLinear"/>

        <Constraint
            android:id="@+id/mainGpsFix"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_marginStart="6dp"
            android:alpha="1"
            app:layout_constraintStart_toEndOf="@+id/mainGpsTitleTv"
            app:layout_constraintTop_toTopOf="@+id/mainGpsTitleTv"
            app:layout_constraintBottom_toBottomOf="@+id/mainGpsTitleTv"/>
        <Constraint
            android:id="@+id/mainSkyStarImg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:alpha="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginBottom="47.5dp"/>
        <Constraint
            android:id="@+id/mainSkyImg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/mainWarningBox"
            app:layout_constraintTop_toBottomOf="@+id/mainMinMaxTr" />
        <Constraint
            android:id="@+id/mainLiveTempValue"
            style="@style/basicMediumTextView"
            android:layout_marginTop="12dp"
            android:lineHeight="52dp"
            android:textSize="72sp"
            app:layout_constraintStart_toStartOf="@+id/mainGpsTitleTv"
            app:layout_constraintTop_toBottomOf="@+id/mainGpsTitleTv" />

        <Constraint
            android:id="@+id/nested_subAir_frame"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/mainMotionSlideGuide"
            android:layout_marginStart="24dp"
            android:layout_marginBottom="25dp"
            android:layout_marginEnd="24dp"
            android:background="@drawable/scrolled_box_bg"
            android:alpha="1"/>

        <Constraint
            android:id="@+id/mainWarningBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/nested_subAir_frame"
            android:background="@drawable/main_bottom_content_box_bg"
            android:layout_marginBottom="12dp"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            android:alpha="1"/>

        <Constraint
            android:id="@+id/mainLiveTempUnit"
            style="@style/basicMediumTextView"
            android:paddingTop="5dp"
            android:text="˚"
            android:alpha="1"
            android:textSize="57sp"
            app:layout_constraintTop_toTopOf="@+id/mainLiveTempValue"
            app:layout_constraintStart_toEndOf="@id/mainLiveTempValue" />
        <Constraint
            android:id="@+id/mainCompareTempTv"
            style="@style/basicMediumTextView"
            android:layout_marginTop="8dp"
            android:textSize="16sp"
            android:alpha="1"
            app:layout_constraintTop_toBottomOf="@id/mainSensTitle"
            app:layout_constraintEnd_toEndOf="@id/mainSensValue"/>

        <Constraint
            android:id="@+id/mainSensTitle"
            style="@style/basicMediumTextView"
            android:textSize="16sp"
            android:alpha="1"
            android:text="체감온도"
            android:layout_marginEnd="13dp"
            app:layout_constraintEnd_toStartOf="@+id/mainSensValue"
            app:layout_constraintTop_toTopOf="@+id/mainSensValue"
            app:layout_constraintBottom_toBottomOf="@+id/mainSensValue"/>

        <Constraint
            android:id="@+id/mainSensValue"
            style="@style/basicMediumTextView"
            android:textSize="16sp"
            android:alpha="1"
            app:layout_constraintEnd_toEndOf="@+id/mainMotionLayout"
            app:layout_constraintTop_toBottomOf="@+id/mainGpsTitleTv"
            android:layout_marginTop="27dp"
            android:layout_marginEnd="24dp"/>

        <Constraint
            android:id="@+id/mainMinMaxTr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"
            android:alpha="1"
            app:layout_constraintTop_toBottomOf="@+id/mainLiveTempValue"
            app:layout_constraintStart_toStartOf="@+id/mainLiveTempValue"
            app:layout_constraintEnd_toEndOf="@+id/mainLiveTempValue" />

        <Constraint
            android:id="@+id/mainPmBoxTL"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="50dp"
            android:alpha="1"
            android:background="@drawable/pm_motion_box_bg"
            android:gravity="center"
            app:layout_constraintTop_toBottomOf="@+id/mainNotiMsgTv" />
        <Constraint
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:layout_marginTop="22dp"
            android:layout_marginEnd="50dp"
            android:alpha="1"
            android:gravity="center"
            app:layout_constraintTop_toBottomOf="@+id/mainPmBoxTL" />
        <Constraint
            android:id="@+id/mainMotionSlideGuide"
            style="@style/basicTextView"
            android:layout_marginBottom="10dp"
            android:alpha="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/mainMotionLinear"
            app:layout_constraintStart_toStartOf="@+id/mainMotionLinear" />
        <Constraint
            android:id="@+id/mainMotionSLideImg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="1"
            app:layout_constraintTop_toTopOf="@+id/mainMotionSlideGuide"
            app:layout_constraintBottom_toBottomOf="@+id/mainMotionSlideGuide"
            app:layout_constraintStart_toEndOf="@+id/mainMotionSlideGuide" />

        <Constraint
            android:id="@+id/nested_fab"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/fab_bg"
            android:src="@drawable/arrow_top"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <Constraint
            android:id="@+id/mainLiveTempTitleC"
            style="@style/basicMediumTextView"
            android:layout_marginStart="17dp"
            android:textSize="16sp"
            android:alpha="0"
            app:layout_constraintEnd_toStartOf="@+id/mainLiveTempValue"
            app:layout_constraintBottom_toBottomOf="@+id/mainLiveTempValue"/>

        <Constraint
            android:id="@+id/mainLiveTempValueC"
            style="@style/basicMediumTextView"
            android:layout_marginStart="17dp"
            android:textSize="18sp"
            android:alpha="0"
            app:layout_constraintEnd_toStartOf="@+id/mainLiveTempValue"
            app:layout_constraintBottom_toBottomOf="@+id/mainLiveTempValue"/>

        <Constraint
            android:id="@+id/mainSensTitleC"
            style="@style/basicTextView"
            android:layout_marginStart="17dp"
            android:textSize="14sp"
            android:text="@string/live_temp"
            android:alpha="0"
            app:layout_constraintEnd_toStartOf="@+id/mainLiveTempValue"
            app:layout_constraintBottom_toBottomOf="@+id/mainLiveTempValue"/>

        <Constraint
            android:id="@+id/mainSensValueC"
            style="@style/basicMediumTextView"
            android:layout_marginStart="17dp"
            android:textSize="18sp"
            android:alpha="0"
            app:layout_constraintEnd_toStartOf="@+id/mainLiveTempValue"
            app:layout_constraintBottom_toBottomOf="@+id/mainLiveTempValue"/>

        <Constraint
            android:id="@+id/mainMinMaxTitleC"
            style="@style/basicTextView"
            android:layout_marginStart="17dp"
            android:textSize="14sp"
            android:text="@string/live_temp"
            android:alpha="0"
            app:layout_constraintEnd_toStartOf="@+id/mainLiveTempValue"
            app:layout_constraintBottom_toBottomOf="@+id/mainLiveTempValue"/>

        <Constraint
            android:id="@+id/mainMinMaxValueC"
            style="@style/basicMediumTextView"
            android:layout_marginStart="17dp"
            android:textSize="18sp"
            android:alpha="0"
            app:layout_constraintEnd_toStartOf="@+id/mainLiveTempValue"
            app:layout_constraintBottom_toBottomOf="@+id/mainLiveTempValue"/>
    </ConstraintSet>




    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/mainTopBarGpsTitleScroll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="1"
            android:layout_marginEnd="15dp"
            app:layout_constraintTop_toTopOf="@+id/topBarLinear"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <Constraint
            android:id="@+id/mainMotionLinear"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:orientation="vertical"
            app:layout_constraintTop_toBottomOf="@+id/topBarLinear" />
        <Constraint
            android:id="@+id/mainGpsTitleTv"
            style="@style/basicTextView"
            android:alpha="0"
            android:fontFamily="@font/spoqa_hansansneo_regular"
            android:gravity="start"
            android:textSize="18sp" />
        <Constraint
            android:id="@+id/nested_subAir_frame"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/mainMotionSlideGuide"
            android:layout_marginStart="24dp"
            android:layout_marginBottom="25dp"
            android:layout_marginEnd="24dp"
            android:background="@drawable/scrolled_box_bg"
            android:alpha="0"/>

        <Constraint
            android:id="@+id/mainWarningBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/nested_subAir_frame"
            android:background="@drawable/main_bottom_content_box_bg"
            android:layout_marginBottom="12dp"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            android:alpha="0"/>

        <Constraint
            android:id="@+id/mainMinMaxTr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"
            android:alpha="0"
            app:layout_constraintTop_toBottomOf="@+id/mainLiveTempValue"
            app:layout_constraintStart_toStartOf="@+id/mainLiveTempValue"
            app:layout_constraintEnd_toEndOf="@+id/mainLiveTempValue" />
        <Constraint
            android:id="@+id/mainGpsFix"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:alpha="0" />
        <Constraint
            android:id="@+id/mainSkyStarImg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:alpha="0"/>
        <Constraint
            android:id="@id/mainSkyImg"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_marginStart="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/mainMotionLinear"
            app:layout_constraintStart_toStartOf="@+id/mainMotionLinear"
            app:layout_constraintTop_toBottomOf="@+id/mainTopBarGpsTitleScroll" />

        <Constraint
            android:id="@+id/mainLiveTempValue"
            style="@style/basicMediumTextView"
            android:alpha="0" />
        <Constraint
            android:id="@+id/mainLiveTempUnit"
            style="@style/basicTextView"
            android:alpha="0"/>
        <Constraint
            android:id="@+id/mainCompareTempTv"
            style="@style/basicMediumTextView"
            android:layout_marginTop="8dp"
            android:alpha="0"
            app:layout_constraintTop_toBottomOf="@id/mainSensTitle"
            app:layout_constraintEnd_toEndOf="@id/mainSensValue"
            android:textSize="16sp" />
        <Constraint
            android:id="@+id/mainSensTitle"
            style="@style/basicTextView"
            android:alpha="0"/>
        <Constraint
            android:id="@+id/mainSensValue"
            style="@style/basicMediumTextView"
            android:alpha="0"/>
        <Constraint
            android:id="@+id/mainPmBoxTL"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="24dp"
            android:alpha="0"
            app:layout_constraintTop_toBottomOf="@+id/mainNotiMsgTv" />
        <Constraint
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            android:layout_marginTop="22dp"
            android:layout_marginEnd="50dp"
            android:alpha="0"
            app:layout_constraintTop_toBottomOf="@+id/mainPmBoxTL" />
        <Constraint
            android:id="@+id/mainMotionSlideGuide"
            android:layout_marginBottom="10dp"
            android:alpha="0"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/mainMotionLinear"
            app:layout_constraintStart_toStartOf="@+id/mainMotionLinear" />
        <Constraint
            android:id="@+id/mainMotionSLideImg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0"
            app:layout_constraintTop_toTopOf="@+id/mainMotionSlideGuide"
            app:layout_constraintBottom_toBottomOf="@+id/mainMotionSlideGuide"
            app:layout_constraintStart_toEndOf="@+id/mainMotionSlideGuide"/>

        <Constraint
            android:id="@+id/nested_fab"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/fab_bg"
            android:src="@drawable/arrow_top"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <Constraint
            android:id="@+id/mainLiveTempTitleC"
            style="@style/basicTextView"
            android:layout_marginStart="25dp"
            android:layout_marginBottom="23dp"
            app:layout_constraintStart_toEndOf="@+id/mainSkyImg"
            app:layout_constraintEnd_toStartOf="@+id/mainSensTitleC"
            app:layout_constraintBottom_toBottomOf="@+id/mainSkyImg" />

        <Constraint
            android:id="@+id/mainLiveTempValueC"
            style="@style/basicMediumTextView"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/mainLiveTempTitleC"
            app:layout_constraintEnd_toEndOf="@+id/mainLiveTempTitleC"
            app:layout_constraintBottom_toTopOf="@+id/mainLiveTempTitleC"/>

        <Constraint
            android:id="@+id/mainSensTitleC"
            style="@style/basicTextView"
            android:textSize="14sp"
            app:layout_constraintStart_toEndOf="@+id/mainSkyImg"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/mainLiveTempTitleC"
            app:layout_constraintBottom_toBottomOf="@+id/mainLiveTempTitleC"/>

        <Constraint
            android:id="@+id/mainSensValueC"
            style="@style/basicMediumTextView"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/mainSensTitleC"
            app:layout_constraintEnd_toEndOf="@+id/mainSensTitleC"
            app:layout_constraintBottom_toTopOf="@+id/mainSensTitleC"/>

        <Constraint
            android:id="@+id/mainMinMaxTitleC"
            style="@style/basicTextView"
            android:textSize="14sp"
            android:layout_marginEnd="25dp"
            app:layout_constraintStart_toEndOf="@+id/mainSensTitleC"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/mainSensTitleC"
            app:layout_constraintBottom_toBottomOf="@+id/mainSensTitleC" />

        <Constraint
            android:id="@+id/mainMinMaxValueC"
            style="@style/basicMediumTextView"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/mainMinMaxTitleC"
            app:layout_constraintEnd_toEndOf="@+id/mainMinMaxTitleC"
            app:layout_constraintBottom_toTopOf="@+id/mainMinMaxTitleC"/>

    </ConstraintSet>
</MotionScene>